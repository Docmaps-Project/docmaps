#################################################################

###  http://github.com/Docmaps-project/docmaps/Docmap
:Docmap rdf:type owl:Class ;
        rdfs:subClassOf pwo:Workflow ;
        rdfs:label "Docmap" .

:DocmapPublisher rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        rdfs:label "DocmapPublisher" .

:DocmapOnlineAccount rdf:type owl:Class ;
        rdfs:subClassOf foaf:OnlineAccount ;
        rdfs:label "DocmapOnlineAccount" .

:DocmapStep rdf:type owl:Class ;
        rdfs:subClassOf pwo:Step ;
        rdfs:label "DocmapStep" .

