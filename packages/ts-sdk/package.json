{
  "name": "docmaps-sdk",
  "version": "0.0.0",
  "repository": "git@github.com:docmaps-project/docmaps.git",
  "homepage": "https://github.com/Docmaps-Project/docmaps/tree/main/packages/ts-sdk",
  "description": "core SDK type codecs and general behaviors for docmaps",
  "main": "dist/docmaps-sdk.cjs.js",
  "module": "dist/docmaps-sdk.esm.js",
  "exports": {
    "require": "./dist/docmaps-sdk.cjs.js",
    "import": "./dist/docmaps-sdk.esm.js"
  },
  "scripts": {
    "test": "ava",
    "debug": "node --loader=ts-node/esm --experimental-specifier-resolution=node --nolazy -r ts-node/register/transpile-only --inspect-brk src/debuggable.ts",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "clean": "rm -rf dist/ node_modules/",
    "build": "tsc",
    "prepublishOnly": "ls -la dist/docmaps-sdk.cjs.js"
  },
  "keywords": [],
  "author": "eve github.com/ships",
  "license": "ISC",
  "files": [
    "dist/",
    "README.md",
    "package.json",
    "tsconfig.json"
  ],
  "dependencies": {
    "@rdfjs/serializer-jsonld-ext": "~4.0.0",
    "fp-ts": "^2.14.0",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "readable-stream": "^4.0.0"
  },
  "devDependencies": {
    "@rdfjs/data-model": "^2.0.1",
    "@rdfjs/parser-n3": "^2.0.1",
    "@rdfjs/types": "^1.1.0",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/jsonld": "^1.5.8",
    "@types/n3": "^1.10.4",
    "@types/node": "^18.16.2",
    "@types/rdf-ext": "^2.0.0",
    "@types/rdfjs__data-model": "^2.0.1",
    "@types/rdfjs__parser-n3": "^2.0.0",
    "@types/rdfjs__serializer-jsonld-ext": "^2.0.5",
    "@types/readable-stream": "^2.3.15",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "ava": "^5.2.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "rdf-ext": "^2.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ],
    "files": [
      "**/*.test.ts"
    ]
  }
}
